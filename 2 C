#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,x,i,even=0,odd=0,neg=0,pos=0;
    cin>>n;
    for(i=1; i<=n; i++)
    {
        cin>>x;
        if(x%2==0)
            even++;
        if(x%2!=0)
            odd++;
        if(x>0)
            pos++;
        if(x<0)
            neg++;
    }
    cout<<"Even: "<<even<<endl;
    cout<<"Odd: "<<odd<<endl;
    cout<<"Positive: "<<pos<<endl;
    cout<<"Negative: "<<neg<<endl;
    return 0;
}
