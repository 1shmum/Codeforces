#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,a,i,x,f=0;
    cin>>n;
    int arr[n];
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    cin>>x;
    for(i=0;i<n;i++)
    {
        if(arr[i]==x)
            {
                cout<<i<<endl;
                f=1;
                break;
            }
    }
    if(f==0)
    cout<<"-1"<<endl;

}
