#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a>=c && b<=d)
        cout<<a<<" "<<b<<endl;
    else if(c<=b && d>b)
        cout<<c<<" "<<b<<endl;
    else if(c>=a && d<=b)
        cout<<c<<" "<<d<<endl;
    else if(c>b || a>d)
        cout<<"-1"<<endl;
    else if(a>=c && d<b)
        cout<<a<<" "<<d<<endl;
    return 0;
}
